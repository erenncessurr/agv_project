<?xml version="1.0" ?>
<sdf version="1.8">
        <!--agv_ÅŸasi-->
        <model name='agv' canonical_link='chassis'>
        <pose relative_to='world'>0 0 0 0 0 0</pose>
            <link name='chassis'>
                <pose relative_to='__model__'>0.5 0 0.32 0 0 0</pose>
                <!--inertial properties of the link mass, inertia matix-->
                <inertial> 
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.15 0.55 0.53</size>
                        </box>
                    </geometry>
                    <!--color black-->
                    <material>
                        <ambient>0 0 0 1</ambient>
                        <diffuse>0 0 0 1</diffuse>
                        <specular>0 0 0 1</specular>
                    </material>
                    <collision name='collision'>
                        <geometry>
                            <box>
                                <size>0.15 0.55 0.53</size>
                            </box>
                        </geometry>
                    </collision>
                </visual>  
                <frame name="lidar_frame" attached_to='chassis'>
                    <pose>0.8 0 0.5 0 0 0</pose>
                </frame>
                <sensor name='gpu_lidar' type='gpu_lidar'>"
                    <pose relative_to='lidar_frame'>0 0 0 0 0 0</pose>
                    <topic>lidar</topic>
                    <update_rate>10</update_rate>
                    <ray>
                        <scan>
                            <horizontal>
                                <samples>640</samples>
                                <resolution>1</resolution>
                                <min_angle>-1.396263</min_angle>
                                <max_angle>1.396263</max_angle>
                            </horizontal>
                            <vertical>
                                <samples>1</samples>
                                <resolution>0.01</resolution>
                                <min_angle>0</min_angle>
                                <max_angle>0</max_angle>
                            </vertical>
                        </scan>
                        <range>
                            <min>0.08</min>
                            <max>10.0</max>
                            <resolution>0.01</resolution>
                        </range>
                    </ray>
                    <always_on>1</always_on>
                    <visualize>true</visualize>
                </sensor>  
            </link>
            <link name='small_box'>
                <pose relative_to='chassis'>-0.12 0 0.16 0 0 0</pose>
                <!--inertial properties of the link mass, inertia matix-->
                <inertial> 
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>0.1 0.1 0.4</size>
                        </box>
                    </geometry>
                    <!--color black-->
                    <material>
                        <ambient>0.35 0.56 0.25 1</ambient>
                        <diffuse>0.35 0.56 0.25 1</diffuse>
                        <specular>0.35 0.56 0.25 1</specular>
                    </material>
                    <collision name='collision'>
                        <geometry>
                            <box>
                                <size>0.1 0.1 0.4</size>
                            </box>
                        </geometry>
                    </collision>
                </visual>                
            </link>
            <!--left fork-->
            <link name='left_fork'>
                <pose relative_to='chassis'>0.65 0.2 -0.2 0 0 0</pose>
                <!--inertial properties of the link mass, inertia matix-->
                <inertial> 
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>1.15 0.15 0.075</size>
                        </box>
                    </geometry>
                    <!--color white-->
                    <material>
                        <ambient>1 1 1 1</ambient>
                        <diffuse>1 1 1 1</diffuse>
                        <specular>1 1 1 1</specular>
                    </material>
                    <collision name='collision'>
                        <geometry>
                            <box>
                                <size>1.15 0.15 0.075</size>
                            </box>
                        </geometry>
                    </collision>
                </visual>                
            </link>     
            <!--right fork-->
            <link name='right_fork'>
                <pose relative_to='chassis'>0.65 -0.2 -0.2 0 0 0</pose>
                <!--inertial properties of the link mass, inertia matix-->
                <inertial> 
                    <mass>1.14395</mass>
                    <inertia>
                        <ixx>0.095329</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.381317</iyy>
                        <iyz>0</iyz>
                        <izz>0.476646</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <box>
                            <size>1.15 0.15 0.075</size>
                        </box>
                    </geometry>
                    <!--color white-->
                    <material>
                        <ambient>1 1 1 1</ambient>
                        <diffuse>1 1 1 1</diffuse>
                        <specular>1 1 1 1</specular>
                    </material>
                    <collision name='collision'>
                        <geometry>
                            <box>
                                <size>1.15 0.15 0.075</size>
                            </box>
                        </geometry>
                    </collision>
                </visual>                
            </link>   
            <!--left wheel-->
            <link name='left_wheel'>
                <pose relative_to="chassis">0.94 0.2 -0.265 0 0 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <sphere>
                            <radius>0.05</radius>
                        </sphere>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <sphere>
                            <radius>0.05</radius>
                        </sphere>
                    </geometry>
                </collision>
            </link>
            <!--right wheel-->
            <link name='right_wheel'>
                <pose relative_to="chassis">0.94 -0.2 -0.265 0 0 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <sphere>
                            <radius>0.05</radius>
                        </sphere>
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>
                        <diffuse>1.0 0.0 0.0 1</diffuse>
                        <specular>1.0 0.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <sphere>
                            <radius>0.05</radius>
                        </sphere>
                    </geometry>
                </collision>
            </link>
            <!--steering-->

            <frame name="steering_frame" attached_to='chassis'>
                <pose>-0.08 0 0.1 0 0 -1.5707</pose>
            </frame>

            <link name='steering_cylinder'>
                <pose relative_to="steering_frame"></pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.01</radius>
                            <length>0.7</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.0 0.0 1.0 1</ambient>
                        <diffuse>0.0 0.0 1.0 1</diffuse>
                        <specular>0.0 0.0 1.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.01</radius>
                            <length>0.7</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <!--front wheel-->
            <link name='steering_wheel'>
                <pose relative_to="steering_cylinder">0 0 -0.37 0 -1.5707 0</pose>
                <inertial>
                    <mass>1</mass>
                    <inertia>
                        <ixx>0.043333</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.043333</iyy>
                        <iyz>0</iyz>
                        <izz>0.08</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.05</radius>
                            <length>0.08</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.0 1.0 0.0 1</ambient>
                        <diffuse>0.0 1.0 0.0 1</diffuse>
                        <specular>0.0 1.0 0.0 1</specular>
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.05</radius>
                            <length>0.08</length>
                        </cylinder>
                    </geometry>
                </collision>
            </link>
            <!--CONNECTIONS-->
            <joint name='left_wheel_joint' type='ball'>
                <pose relative_to='left_wheel'/>
                <parent>chassis</parent>
                <child>left_wheel</child>
                <!--<axis>-->
                    <!--<xyz expressed_in='__model__'>0 1 0</xyz>-->
                    <!--<limit>-->
                        <!--<lower>-1.79769e+308</lower>-->    <!--negative infinity-->
                        <!--<upper>1.79769e+308</upper> -->    <!--positive infinity-->
                    <!--</limit>-->
                <!--</axis>-->
            </joint>

            <joint name='right_wheel_joint' type='ball'>
                <pose relative_to='right_wheel'/>
                <parent>chassis</parent>
                <child>right_wheel</child>
                <!-- <axis> -->
                    <!--<xyz expressed_in='__model__'>0 1 0</xyz>-->
                    <!--<limit>-->
                        <!--<lower>-1.79769e+308</lower>-->    <!--negative infinity-->
                        <!--<upper>1.79769e+308</upper> -->    <!--positive infinity-->
                    <!--</limit>-->
                <!--</axis>-->
            </joint>

            <joint name='steering_cylinder_joint' type='revolute'>
                <pose relative_to='steering_cylinder'/>
                <parent>chassis</parent>
                <child>steering_cylinder</child>
                <axis>
                    <xyz expressed_in='__model__'>0 0 1</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>  <!--negative infinity-->
                        <upper>1.79769e+308</upper>    <!--positive infinity-->
                    </limit>
                </axis>
            </joint>

            <joint name='steering_wheel_joint' type='revolute'>
                <pose relative_to='steering_wheel'/>
                <parent>chassis</parent>
                <child>steering_wheel</child>
                <axis> 
                   <xyz expressed_in='__model__'>0 1 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>  <!--negative infinity-->
                        <upper>1.79769e+308</upper>    <!--positive infinity-->
                    </limit>
                </axis>                
            </joint>

            <joint name='right_fork_joint' type='fixed'>
                <pose relative_to='right_fork'/>
                <parent>chassis</parent>
                <child>right_fork</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz>
                </axis>
            </joint>

            <joint name='left_fork_joint' type='fixed'>
                <pose relative_to='left_fork'/>
                <parent>chassis</parent>
                <child>left_fork</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz>
                </axis>
            </joint>

            <joint name='small_box_joint' type='fixed'>
                <pose relative_to='small_box'/>
                <parent>chassis</parent>
                <child>small_box</child>
                <axis>
                    <xyz expressed_in='__model__'>0 1 0</xyz>
                </axis>
            </joint>

            <!--diff drive plugin-->
            <plugin
                filename="libignition-gazebo-diff-drive-system.so"
                name="ignition::gazebo::systems::DiffDrive">
                <left_joint>steering_cylinder_joint</left_joint>
                <right_joint>steering_wheel_joint</right_joint>
                <wheel_separation>0.4</wheel_separation>
                <wheel_radius>0.05</wheel_radius>
                <odom_publish_frequency>1</odom_publish_frequency>
                <topic>cmd_vel</topic>
            </plugin>
        </model>

        <!-- Moving Forward-->
        <plugin filename="libignition-gazebo-triggered-publisher-system.so"
                name="ignition::gazebo::systems::TriggeredPublisher">
            <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
                <match field="data">16777235</match>
            </input>
            <output type="ignition.msgs.Twist" topic="/cmd_vel">
                linear: {x: 0.5}, angular: {z: 0.0}
            </output>
        </plugin>

        <!-- Moving Backward-->
        <plugin filename="libignition-gazebo-triggered-publisher-system.so"
                name="ignition::gazebo::systems::TriggeredPublisher">
            <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
                <match field="data">16777237</match>
            </input>
            <output type="ignition.msgs.Twist" topic="/cmd_vel">
                linear: {x: -0.5}, angular: {z: 0.0}
            </output>
        </plugin>

        <!-- Rotating right-->
        <plugin filename="libignition-gazebo-triggered-publisher-system.so"
                name="ignition::gazebo::systems::TriggeredPublisher">
            <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
                <match field="data">16777236</match>
            </input>
            <output type="ignition.msgs.Twist" topic="/cmd_vel">
                linear: {x: 0.0}, angular: {z: -0.5}
            </output>
        </plugin>

        <!--Rotating left-->
        <plugin filename="libignition-gazebo-triggered-publisher-system.so"
                name="ignition::gazebo::systems::TriggeredPublisher">
            <input type="ignition.msgs.Int32" topic="/keyboard/keypress">
                <match field="data">16777234</match>
            </input>
            <output type="ignition.msgs.Twist" topic="/cmd_vel">
                linear: {x: 0.0}, angular: {z: 0.5}
            </output>
        </plugin>
</sdf>